{
  "name": "js-dev-starter",
  "version": "1.0.0",
  "description": "Javascript Development Starter Kit",
  "main": "src/main.ts",
  "scripts": {
    "build:dev": "npm run clean:dist && npm test && webpack --config config/server/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && npm test && webpack --config config/server/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "nsp": "nsp check",
    "postinstall": "npm run nsp",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "rimraf": "rimraf",
    "server:dev": "npm run build:dev && NODE_ENV=dev PORT=3000 node dist/main.bundle.js",
    "server:prod": "npm run build:prod && NODE_ENV=prod PORT=8000 node dist/main.bundle.js",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "npm run lint && npm run nsp && mocha --reporter spec config/mocha.config.js \"src/**/*.spec.ts\"",
    "tslint": "tslint",
    "version": "npm run build",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --watch",
    "watch": "npm run watch:dev",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rharris2825/js-dev-starter.git"
  },
  "keywords": [
    "Development",
    "Starter",
    "Kit"
  ],
  "author": "John Robert Harris II",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rharris2825/js-dev-starter/issues"
  },
  "homepage": "https://github.com/rharris2825/js-dev-starter#readme",
  "devDependencies": {
    "@types/body-parser": "0.0.34",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "0.0.30",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.35",
    "@types/jsdom": "^2.0.30",
    "@types/mocha": "^2.2.39",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "@types/open": "0.0.29",
    "awesome-typescript-loader": "^3.0.4",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "mocha": "^3.2.0",
    "nsp": "^2.6.2",
    "rimraf": "^2.6.0",
    "ts-babel-node": "^1.1.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "morgan": "^1.8.1"
  }
}
