{
  "name": "js-dev-starter",
  "version": "1.0.0",
  "description": "Javascript Development Starter Kit",
  "main": "app.js",
  "scripts": {
    "build": "npm-run-all lint test webpack",
    "test": "mocha --reporter progress ./build-scripts/test-setup.js \"src/**/*.spec.ts\"",
    "test:watch": "npm run test -- --watch",
    "prestart": "babel-node ./build-scripts/start-message.js",
    "start": "npm-run-all --parallel  lint:watch test:watch security-check start-dev-server",
    "start-dev-server": "babel-node ./build-scripts/dev-server.js",
    "security-check": "babel-node ./build-scripts/security-check.js",
    "localtunnel": "babel-node ./build-scripts/localtunnel.js",
    "share": "npm-run-all --parallel start-dev-server localtunnel",
    "lint": "babel-node ./build-scripts/lint.js",
    "lint:watch": "babel-node ./build-scripts/lint-watch.js",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rharris2825/js-dev-starter.git"
  },
  "keywords": [
    "Development",
    "Starter",
    "Kit"
  ],
  "author": "John Robert Harris II",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rharris2825/js-dev-starter/issues"
  },
  "homepage": "https://github.com/rharris2825/js-dev-starter#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/chalk": "^0.4.31",
    "@types/express": "^4.0.35",
    "@types/jsdom": "^2.0.30",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/open": "0.0.29",
    "awesome-typescript-loader": "^3.0.4",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "chokidar": "^1.6.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.14.1",
    "glob": "^7.1.1",
    "jsdom": "^9.11.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "ts-babel-node": "^1.1.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {},
  "watch": {
    "lint": "src/**/*.ts"
  }
}
